{
  "extends": "airbnb-base",
  "plugins": [
    "import"
  ],
  "env": {
    "browser": true,
    "mocha": true,
    "node": true,
    "es6": true,
    "jquery": true
  },
  "rules": {
    "quotes": [2, "single"],
    "eol-last": [0],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0],
    "consistent-return": [0],
    "arrow-body-style": [0],
    "camelcase": [0],
    "no-shadow": [0],
    "no-multi-assign": [0],
    "no-param-reassign": [0],
    "no-else-return": [0],
    "no-unused-expressions": [0],
    "no-unused-vars": [0]
  }
}
